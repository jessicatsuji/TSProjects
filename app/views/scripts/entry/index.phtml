<h1>Ts Projects</h1>

<div class="entryForm">
    <form action="/entry/create" method="post">
    <label>title:</label>
    <input type="text" class="text" name="title" id="title"/>
    <br />
    <label>URL:</label>
    <input type="text" class="text" name="url" id="url"/>
    <br />
    <label>project authors:</label>
    <input type="text" class="text" name="authors" id="authors" />
    <br /><br /><br />
    <label>class:</label>
    <input type="text" class="text" name="courses" id="courses" />
    <br />
<span class="tools">
    <label>tools:</label>
    <br />
    Photoshop
    <input type="checkbox" name="photoshop" id="photoshop"/>
    <br />
    Flash
    <input type="checkbox" name="flash" id="flash"/>
    <br />
    Illustrator
    <input type="checkbox" name="illustrator" id="illustrator"/>
    <br />
    Dreamweaver
    <input type="checkbox" name="dreamweaver" id="dreamweaver"/>
    <br />
    Coda
    <input type="checkbox" name="coda" id="coda"/>
    <br />
    Text Mate
    <input type="checkbox" name="textmate" id="textmate"/>
    <br />
    jQuery
    <input type="checkbox" name="jquery" id="jquery"/>
    <br />
</span>
<span class="language">
    <label>languages:</label>
    <br />
    HTML
    <input type="checkbox" name="html" id="html"/>
    <br />
    CSS
    <input type="checkbox" name="css" id="css"/>
    <br />
    XML
    <input type="checkbox" name="xml" id="xml"/>
    <br />
    Javascript
    <input type="checkbox" name="javascript" id="javascript"/>
    <br />
    PHP
    <input type="checkbox" name="php" id="php"/>
    <br />
    Actionscript
    <input type="checkbox" name="actionscript" id="actionscript"/>
</span>
<span class="clear"></span>
    <br />
    <label>date:</label>
    <input type="text" class="text" name="date_complete" id="date_complete"/>
    <br /><br /><br />
    <label>assignment specs:</label>
    <textarea name="assigmentSpec" rows="4" cols="60" name="assign_spec" id="assign_spec"></textarea>
    <br />
    <label>project approach:</label>
    <textarea name="projectApproach" rows="4" cols="60" name="project_approach" id="project_approach"></textarea>
    <br />
    <br />
    <input type="submit" value="submit" id="submit"/>
    </form>
</div>